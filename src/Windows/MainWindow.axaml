<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" x:Name="Myself"
		MinWidth="300" MinHeight="250"
		Width="300" Height="250"
        x:Class="Leayal.SnowBreakLauncher.MainWindow"
        Title="SnowBreak: Containment Break GameLauncher">
	<Grid ColumnDefinitions="*,Auto" RowDefinitions="*,Auto">
		<Button Grid.Row="1" Grid.Column="1" Margin="2" x:Name="Btn_StartGame" Click="Btn_StartGame_Click">
			<TextBlock Name="BtnText_StartGame" Text="Loading launcher UI..." />
		</Button>
	</Grid>
</Window>
